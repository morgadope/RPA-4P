    "from datetime import timedelta,datetime\r\n",
    "\r\n",
    "\r\n",
    "data = datetime.now()\r\n",
    "\r\n",
    "if datetime.today().weekday() !=0:\r\n",
    "    data=+timedelta(days=-1)\r\n",
    "else:\r\n",
    "    data=+timedelta(days=-2)\r\n",
    "    data=(format(data,\"%d/%m/%Y\"))\r\n",
    "    print(data)"
